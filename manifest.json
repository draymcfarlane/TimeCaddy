{
    "manifest_version": 2,
    "name": "Time Management Extension",
    "version": "1.0",
    "description": "Track and limit time spent on websites",
    "permissions": [
      "storage",
      "tabs",
      "alarms"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }